{
  "name": "reactchartiq",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-create-action-types": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "esformatter-jsx": "^7.2.0",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0-0",
    "eslint-plugin-standard": "^2.0.1",
    "prop-types": "^15.6.0",
    "redux-devtools": "^3.4.1",
    "webpack": "^2.2.1"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": false,
      "maxAttrsOnTag": 3,
      "firstAttributeOnSameLine": true,
      "formatJSXExpressions": true,
      "JSXExpressionsSingleLine": true,
      "alignWithFirstAttribute": false,
      "spaceInJSXExpressionContainers": " ",
      "removeSpaceBeforeClosingJSX": false,
      "closingTagOnNewLine": false,
      "htmlOptions": {}
    }
  },
  "scripts": {
    "build": "webpack --config=webpack.config.js",
    "postinstall": "webpack",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base . --watch=true",
    "start": "NODE_ENV=development webpack-dev-server  --config=webpack.build.config.js --open --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChartIQ/Charting-Library---React-Seed-Project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project/issues"
  },
  "homepage": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project#readme"
}
